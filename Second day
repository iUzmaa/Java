import java.util.*;
class main{
    public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    if(a==b)
    {
        System.out.println("Both are equal!");
    }
    else if(a>b)
    {
        
            System.out.println("A is Greater");
        }
        else{
            System.out.println("A is Lesser");
        }
        
    }
    
    }

import java.util.*;
class HelloWorld {
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        if(age>18)
        {
            System.out.println("Yes u r an Adult");
        }
        else
        {
            System.out.println("No U r not an adult");
        }
        
    }
}


import java.util.*;
class Second
{
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int num =  sc.nextInt();
        if ( num%2==0)
        {
            System.out.println("EVen");
        }
        else
        {
            System.out.println("Odd");
        }
    }
}













import java.util.*;
class main{
    public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int b = sc.nextInt();
    if(a==b)
    {
        System.out.println("Both are equal!");
    }
    else
    {
        if(a>b)
        {
            System.out.println("A is Greater");
        }
        else{
            System.out.println("A is Lesser");
        }
        
    }
    
    }
}

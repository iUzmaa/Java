import java.util.*;
class HelloWorld {
    public static void main(String args[])
    {
        Scanner sc = new Scanner(System.in);
        int age = sc.nextInt();
        if(age>18)
        {
            System.out.println("Yes u r an Adult");
        }
        else
        {
           System.out.println("No U r not an adult");
        }
        
    }
}


